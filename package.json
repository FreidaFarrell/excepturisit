    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "uglify-js": "^2.4.15",
    "watchify": "^3.4.0"
  },
  "browserify": {
